{
    "name": "winedering/yii2-user",
    "description": "Winedering maintained fork of dektrium/yii2-user. Unified login and registration, bugfixes, and modern PHP support.",
    "keywords": [
        "yii2",
        "yii2-user",
        "user",
        "authentication",
        "registration",
        "dektrium",
        "winedering"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/winedering/yii2-user/issues",
        "source": "https://github.com/winedering/yii2-user"
    },
    "authors": [
        {
            "name": "Dmitry Erofeev (original author)",
            "email": "dmeroff@gmail.com"
        },
        {
            "name": "Winedering Team",
            "email": "tech@winedering.com",
            "homepage": "https://www.winedering.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "^2.0.13",
        "yiisoft/yii2-swiftmailer": "^2.0",
        "yiisoft/yii2-authclient": "^2.1",
        "yiisoft/yii2-bootstrap": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "dektrium\\user\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dektrium\\user\\tests\\": "tests/"
        }
    },
    "extra": {
        "bootstrap": "dektrium\\user\\Bootstrap",
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "replace": {
        "dektrium/yii2-user": "self.version"
    },
    "config": {
        "sort-packages": true
    }
}
